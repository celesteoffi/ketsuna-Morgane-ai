{
  "name": "ketsuna",
  "version": "1.0.0",
  "description": "Ketsuna Discord Bot",
  "source": "dist/main.js",
  "main": "out/bundle.js",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && swc src --out-dir dist && parcel build",
    "start": "cross-env NODE_ENV=production node out/bundle.js"
  },
  "author": "garder500",
  "engines": {
    "node": ">=20"
  },
  "targets": {
    "main": {
      "includeNodeModules": {
        "react": false,
        "react-dom": false,
        "@emotion/react": false,
        "@emotion/styled": false,
        "@mui/icons-material": false,
        "@mui/material": false
      }
    }
  },
  "resolvers": {
    "@parcel/resolver-default": {
      "packageExports": true,
      "modules": [
        "node_modules"
      ]
    }
  },
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "@types/centra": "^2.2.3",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "cross-env": "^7.0.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "@thunder04/supermap": "^3.0.3",
    "centra": "^2.6.0",
    "discord-interactions": "^3.4.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.1",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
