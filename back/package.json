    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "nodemon": "^3.0.2"
  },
  "dependencies": {
    "@types/centra": "^2.2.3",
    "centra": "^2.6.0",
    "discord-interactions": "^3.4.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "fastify": "^4.25.1"
  }
}
